{
  "id": "room_get",
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "pattern": "^[\\w0-9]{8}(?:-[\\w0-9]{4}){3}-[\\w0-9]{12}$",
      "required": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "nickname": {
      "type": "string",
      "required": true
    },
    "isPrivate": {
      "type": "boolean",
      "required": true,
      "default": false
    }
  }
}